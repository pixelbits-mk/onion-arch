{
  "name": "products-adapters-delivery",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/products/adapters/delivery/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/products/adapters/delivery",
        "tsConfig": "libs/products/adapters/delivery/tsconfig.lib.json",
        "packageJson": "libs/products/adapters/delivery/package.json",
        "main": "libs/products/adapters/delivery/src/index.ts",
        "assets": ["libs/products/adapters/delivery/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/products/adapters/delivery/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/products/adapters/delivery/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": ["adapter"]
}
